<%= [:order_number , :ordered_on ,:total_price , :vat , :vat , :paid_on ].collect{|i| I18n.t(i)}.join(";") %>
<% @orders.each do |order| %>
<% values = [ order.number.to_s , date(order.ordered_on) ,  order.total_price.round(2)]  %>
<% taxes = order.taxes %>
<% values << taxes[14.0]   %>              
<% values << taxes[24.0] %>
<% values << date(order.paid_on) %>              
<%= values.join(";")   %>              
<%end%>
